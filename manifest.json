{
    "name": "WUSTL AutoReg",
    "description": "AutoReg Extension for WashU WebSTAC",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./images/bigchecklogo.png",
        "32": "./images/bigchecklogo.png",
        "48": "./images/bigchecklogo.png",
        "128": "./images/bigchecklogo.png"
    },
    "background": {
        "scripts": [
            "./js/background.js"
        ]
    },
    "browser_action": {
        "default_popup": "./popup.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "*://*.wustl.edu/*"
    ]



}